#include <stdio.h>


// Function to return the maximum of four integers
int max_of_four(int a, int b, int c, int d) {
    int max = a;
    if (b > max) max = b;
    if (c > max) max = c;
    if (d > max) max = d;
    return max;
}

int main() {
    int a, b, c, d;
    // Reading four integers from the user
    scanf("%d %d %d %d", &a, &b, &c, &d);
    
    // Finding the maximum and printing it
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}

